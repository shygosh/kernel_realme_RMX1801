&tlmm {
	oppo_fp_common {
		gpio_id1_default: gpio_id1_default {
			mux {
				pins = "gpio62";
				function = "gpio";
			};
			config {
				pins = "gpio62";
				drive-strength = <2>;
				bias-pull-up;
				input-enable;
			};
		};

		gpio_id2_default: gpio_id2_default {
			mux {
				pins = "gpio74";
				function = "gpio";
			};
			config {
				pins = "gpio74";
				drive-strength = <2>;
				bias-pull-up;
				input-enable;
			};
		};

		gpio_id3_default: gpio_id3_default {
			mux {
				pins = "gpio77";
				function = "gpio";
			};
			config {
				pins = "gpio77";
				drive-strength = <2>;
				bias-pull-up;
				input-enable;
			};
		};
	};

	fpc_fpc1020 {
		gpio_vdd_en_default: gpio_vdd_en_default {
			mux {
				pins = "gpio40";
				function = "gpio";
			};
			config {
				pins = "gpio40";
				drive-strength = <2>;
				bias-pull-down;
				output-low;
			};
		};

		gpio_fpc_irq_default: gpio_fpc_irq_default {
			mux {
				pins = "gpio72";
				function = "gpio";
			};
			config {
				pins = "gpio72";
				drive-strength = <2>;
				bias-pull-up;
				input-enable;
			};
		};
	};

	goodix_fp {
		gpio_goodix_irq_default: gpio_goodix_irq_default {
			mux {
				pins = "gpio72";
				function = "gpio";
			};
			config {
				pins = "gpio72";
				drive-strength = <2>;
				bias-disable;
				input-enable;
			};
		};
	};

	/* active state */
	hw_sub_gpio_active: hw_sub_gpio_active {
		mux {
			pins = "gpio30", "gpio31";
			function = "gpio";
		};
		config {
			pins = "gpio30", "gpio31";
			drive-strength = <2>; /* 2 MA */
			bias-pull-up;
		};
	};

	/* sleep state */
	hw_sub_gpio_sleep: hw_sub_gpio_sleep {
		mux {
			pins = "gpio30", "gpio31";
			function = "gpio";
		};
		config {
			pins = "gpio30", "gpio31";
			drive-strength = <2>; /* 2 MA */
			bias-pull-down; /* PULL DOWN */
		};
	};

	sim1_cd_on: sim1_cd_on {
		mux {
			pins = "gpio90";
			function = "gpio";
		};
		config {
			pins = "gpio90";
			bias-pull-up; /* pull up */
			drive-strength = <2>; /* 2 MA */
		};
	};

	sim1_cd_off: sim1_cd_off {
		mux {
			pins = "gpio90";
			function = "gpio";
		};
		config {
			pins = "gpio90";
			bias-pull-up; /* pull up */
			drive-strength = <2>; /* 2 MA */
		};
	};
	
	sim2_cd_on: sim2_cd_on {
		mux {
			pins = "gpio86";
			function = "gpio";
		};
		config {
			pins = "gpio86";
			bias-pull-up; /* pull up */
			drive-strength = <2>; /* 2 MA */
		};
	};

	sim2_cd_off: sim2_cd_off {
		mux {
			pins = "gpio86";
			function = "gpio";
		};
		config {
			pins = "gpio86";
			bias-pull-up; /* pull up */
			drive-strength = <2>; /* 2 MA */
		};
	};

	pmx_ts_int_active {
		ts_int_active: ts_int_active {
			mux {
				pins = "gpio67";
				function = "gpio";
			};
			config {
				pins = "gpio67";
				drive-strength = <8>;
				bias-pull-up;
			};
		};
	};

	pmx_ts_reset_active {
		ts_reset_active: ts_reset_active {
			mux {
				pins = "gpio66";
				function = "gpio";
			};
			config {
				pins = "gpio66";
				drive-strength = <8>;
				bias-disable;
			};
		};
	};

	mdss_exits_active: mdss_exits_active {
		mux {
			pins = "gpio73";
			function = "gpio";
		};
		config {
			pins = "gpio73";
			drive-strength = <2>; /* 2 mA */
			bias-disable = <0>; /* no pull */
			input-enable;
		};
	};

	mdss_exits_suspend: mdss_exits_suspend {
		mux {
			pins = "gpio73";
			function = "gpio";
		};
		config {
			pins = "gpio73";
			drive-strength = <2>; /* 2 mA */
			bias-pull-down; /* pull down */
			input-enable;
		};
	};

	mdss_err_active: mdss_err_active {
		mux {
			pins = "gpio56";
			function = "gpio";
		};
		config {
			pins = "gpio56";
			drive-strength = <2>; /* 2 mA */
			bias-pull-down; /* pull down*/
			input-enable;
		};
	};

	mdss_err_suspend: mdss_err_suspend {
		mux {
			pins = "gpio56";
			function = "gpio";
		};
		config {
			pins = "gpio56";
			drive-strength = <2>; /* 2 mA */
			bias-pull-down; /* pull down */
			input-enable;
		};
	};

	mdss_poc_1p8_active: mdss_poc_1p8_active {
		mux {
			pins = "gpio63";
			function = "gpio";
		};
		config {
			pins = "gpio63";
			drive-strength = <2>;
			bias-pull-up;
			output-high;
		};
	};

	mdss_poc_1p8_suspend: mdss_poc_1p8_suspend {
		mux {
			pins = "gpio63";
			function = "gpio";
		};
		config {
			pins = "gpio63";
			drive-strength = <2>;
			bias-disable;
			output-low;
		};
	};

	charger: charger {
		charging_switch1_active: charging_switch1_active {
			mux {
				pins = "gpio75";
				function = "gpio";
			};
			config {
				pins = "gpio75";
				drive-strength = <2>;
				bias-pull-up;
			};
		};

		charging_switch1_sleep: charging_switch1_sleep {
			mux {
				pins = "gpio75";
				function = "gpio";
			};
			config {
				pins = "gpio75";
				drive-strength = <2>;
				bias-pull-down;
			};
		};

		charging_reset_active: charging_reset_active {
			mux {
				pins = "gpio55";
				function = "gpio";
			};
			config {
				pins = "gpio55";
				drive-strength = <2>;
				bias-pull-up;
			};
		};

		charging_reset_sleep: charging_reset_sleep {
			mux {
				pins = "gpio55";
				function = "gpio";
			};
			config {
				pins = "gpio55";
				drive-strength = <2>;
				bias-pull-down;
			};
		};

		charging_clock_active: charging_clock_active {
			mux {
				pins = "gpio51";
				function = "gpio";
			};
			config {
				pins = "gpio51";
				drive-strength = <2>;
				bias-pull-up;
			};
		};

		charging_clock_sleep: charging_clock_sleep {
			mux {
				pins = "gpio51";
				function = "gpio";
			};
			config {
				pins = "gpio51";
				drive-strength = <2>;
				bias-pull-down;
			};
		};

		charging_data_active: charging_data_active {
			mux {
				pins = "gpio44";
				function = "gpio";
			};
			config {
				pins = "gpio44";
				drive-strength = <2>;
				bias-pull-up;
			};
		};

		charging_data_sleep: charging_data_sleep {
			mux {
				pins = "gpio44";
				function = "gpio";
			};
			config {
				pins = "gpio44";
				drive-strength = <2>;
				bias-pull-down;
			};
		};

		chargerid_switch_active: chargerid_switch_active {
			mux {
				pins = "gpio65";
				function = "gpio";
			};
			config {
				pins = "gpio65";
				drive-strength = <2>;
				bias-pull-up;
			};
		};

		chargerid_switch_sleep: chargerid_switch_sleep {
			mux {
				pins = "gpio65";
				function = "gpio";
			};
			config {
				pins = "gpio65";
				drive-strength = <2>;
				bias-pull-down;
			};
		};

		chargerid_switch_default: chargerid_switch_default {
			mux {
				pins = "gpio65";
				function = "gpio";
			};
			config {
				pins = "gpio65";
				drive-strength = <2>;
				bias-disable; /* NO PULL */
			};
		};

		usbid_active: usbid_active {
			mux {
				pins = "gpio49";
				function = "gpio";
			};
			config {
				pins = "gpio49";
				drive-strength = <2>;
				input-enable;
				bias-pull-up; /*PULL UP*/
			};
		};

		usbid_sleep: usbid_sleep {
			mux {
				pins = "gpio49";
				function = "gpio";
			};
			config {
				pins = "gpio49";
				drive-strength = <2>;
				output-low;
				bias-pull-down; /*PULL DOWN*/
			};
		};
		
		shortc_active: shortc_active {
			mux {
				pins = "gpio60";
				function = "gpio";
			};
			config {
				pins = "gpio60";
				drive-strength = <2>;
				input-enable;
				bias-pull-up; /*PULL UP*/
			};
		};
	};

	/delete-node/ tlmm_gpio_key;
};

&sdc1_clk_on {
	config {
		drive-strength = <10>; /* 10 MA */
	};
};

